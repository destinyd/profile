#!/bin/bash
a=$(/home/dd/.conkycolors/bin/lunar);
#echo $a;
b=$(echo $a | awk '{print substr($1,length($a)-5,8)}');
#echo $b;
case $b in
   "十四"|"十五"|"十六") c="K" ;;
   "十二"|"十三"|"十七"|"十八") c="L" ;;
   "初十"|"十一"|"十九"|"二十"|"廿一") c="M" ;;
   "初七"|"初八"|"初九"|"廿二"|"廿三"|"廿四") c="N" ;;
   "初四"|"初五"|"初六"|"廿五"|"廿六"|"廿七") c="O" ;;
   "初一"|"初二"|"初三"|"廿八"|"廿九"|"三十") c="P" ;;
   *)
esac
echo $c;
