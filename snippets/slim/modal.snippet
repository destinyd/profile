#${1:myModal}.modal.fade tabindex=-1 role="dialog" aria-labelledby="${2:myModalLabel}"
  .modal-dialog
    .modal-content
      .modal-header
        button.close type="button" data-dismiss="modal" aria-label="关闭"
          span aria-hidden="true" &times;
        h4#${2:myModalLabel}.modal-title ${3:标题}

      .modal-body
	${4:内容}

      .modal-footer
        button.btn.btn-default type="button" data-dismiss="modal" 关闭
        button type="button" class="btn btn-primary" 提交
